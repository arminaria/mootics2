<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/2.2" fx:controller="modules.importWizard.controller.MaterialImportController">
    <Text text="Gather Material Information">
        <VBox.margin>
            <Insets bottom="50.0" fx:id="x1"/>
        </VBox.margin>
    </Text>
    <Text text="This Wizard will lead you to the Moodle page to gather information.">
        <VBox.margin>
            <Insets bottom="5.0"/>
        </VBox.margin>
    </Text>
    <Text text="Start Browser with the Button and follow the instructions at the top of the page." VBox.margin="$x1"/>
    <Button mnemonicParsing="false" onAction="#start" prefHeight="80.0" prefWidth="150.0" text="Start Browser"/>
    <TextArea fx:id="output" editable="false">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
    </TextArea>
    <padding>
        <Insets bottom="15.0" top="15.0"/>
    </padding>
</VBox>
