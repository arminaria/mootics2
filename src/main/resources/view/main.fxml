<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="438.0" prefWidth="586.0" xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.MainController">
    <!-- Menu -->
    <top>
        <VBox prefHeight="25.0" prefWidth="337.0">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="Data">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Import Wizard"
                                          onAction="#gotoImportWizard"/>
                                <MenuItem mnemonicParsing="false" text="Backup"
                                          onAction="#gotoBackUp"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Analytics">
                            <items>
                                <MenuItem mnemonicParsing="false" text="UserProfile"
                                          onAction="#gotoUserProfile"/>
                                <MenuItem mnemonicParsing="false" text="Monthly Usage"
                                          onAction="#gotoMonthlyUsage"/>
                                <MenuItem mnemonicParsing="false" text="Statistics"
                                          onAction="#gotoStatistics"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </VBox>
    </top>

</BorderPane>
